{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  // 数据库连接字符串
  "ConnectionSettings": {
    // 默认库连接Id
    "ConnectionId": "Gejia.Gateway",
    // 默认库服务器Ip
    "ServiceIp": "192.168.1.22",
    // 默认库端口号
    "Port": "1433",
    // 默认库数据库名称
    "DbName": "Gejia.Gateway",
    // 默认库数据库用户
    "DbUser": "sa",
    // 默认库数据库密码
    "DbPwd": "dev.2018",
    // 默认库服务器类型
    "DbType": "SqlServer", // MySql 0，SqlServer 1，Sqlite 2，Oracle 3
    // 超时时间，单位秒
    "CommandTimeOut": 60,
    // SqlSugar Sql执行最大秒数，如果超过记录警告日志
    "SugarSqlExecMaxSeconds": 30,
    // 差异日志
    "DiffLog": true
  },
  // 雪花ID
  "SnowflakeSettings": {
    "WorkerId": 1 // 取值范围0~63,默认1
  }
}
