{
  // 数据库连接字符串
  "ConnectionSettings": {
    // 默认库连接Id
    "ConnectionId": "Fast",
    // 默认库服务器Ip
    "ServiceIp": "127.0.0.1",
    // 默认库端口号
    "Port": "1433",
    // 默认库数据库名称
    "DbName": "Fast.Core",
    // 默认库数据库用户
    "DbUser": "sa",
    // 默认库数据库密码
    "DbPwd": "123456",
    // 默认库服务器类型
    "DbType": "SqlServer", // MySql 0，SqlServer 1，Sqlite 2，Oracle 3
    // 超时时间，单位秒
    "CommandTimeOut": 60,
    // SqlSugar Sql执行最大秒数，如果超过记录警告日志
    "SugarSqlExecMaxSeconds": 30,
    // 差异日志
    "DiffLog": true,
    // 禁用 SqlSugar 的 Aop
    "DisableAop": false
  },
  // Redis缓存连接字符串
  "RedisConnectionString": "127.0.0.1:6379,password=123456,defaultDatabase=2",
  // MQ
  "RabbitMQConnection": {
    "HostName": "127.0.0.1",
    "Port": 5672,
    "UserName": "admin",
    "Password": "123456"
  },
  // 雪花ID
  "SnowId": {
    "WorkerId": "1" // 取值范围0~63,默认1
  }
}
