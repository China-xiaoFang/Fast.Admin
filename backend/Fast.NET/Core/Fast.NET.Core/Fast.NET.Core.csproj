<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>Fast.NET 框架核心模块库。</Description>
	</PropertyGroup>

	<ItemGroup>
		<!-- 阻止将内部资源文件夹共享到每个类库的 CommunalResources 文件夹下面 -->
		<Compile Remove="$(SolutionDir)Core\Fast.NET.Core\InternalResources\**\*.cs" />
		<None Remove="$(SolutionDir)Core\Fast.NET.Core\InternalResources\*.md" />
	</ItemGroup>

	<ItemGroup>
		<!-- 将内部资源文件夹共享到每个类库的 InternalResources 文件夹下面 --> 
		<Compile Include="$(SolutionDir)Core\Fast.NET.Core\InternalResources\**\*.cs" />
		<None Include="$(SolutionDir)Core\Fast.NET.Core\InternalResources\*.md" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Include="InternalResources\Definition\UserAgentInfo.cs" />
	  <Compile Include="InternalResources\Definition\WanNetIPInfo.cs" />
	</ItemGroup>

</Project>