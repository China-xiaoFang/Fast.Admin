<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>Fast.NET 框架规范化处理模块库。</Description>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Assets\error.html" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Assets\error.html" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Exceptions\UserFriendlyException.cs">
	    <PackagePath>\</PackagePath>
	    <Pack>true</Pack>
	  </Compile>
	  <Compile Update="Extensions\ExceptionExtension.cs">
	    <PackagePath>\</PackagePath>
	    <Pack>true</Pack>
	  </Compile>
	  <Compile Update="Metadatas\ExceptionMetadata.cs">
	    <PackagePath>\</PackagePath>
	    <Pack>true</Pack>
	  </Compile>
	  <Compile Update="Metadatas\ValidationMetadata.cs">
	    <PackagePath>\</PackagePath>
	    <Pack>true</Pack>
	  </Compile>
	</ItemGroup>

</Project>