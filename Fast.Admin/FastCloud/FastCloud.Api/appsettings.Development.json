{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Fast": {
      // 文件格式
      "FileFormat": "/{0:yyyy}-{0:MM}_{0:dd}",
      // 最小日志级别
      "MiniLogLevel": "Information"
    }
  },
  // 数据库连接字符串
  "ConnectionSettings": {
    // 默认库服务器Ip
    "ServiceIp": "127.0.0.1",
    // 默认库端口号
    "Port": "1433",
    // 默认库数据库用户
    "DbUser": "admin",
    // 默认库数据库密码
    "DbPwd": "123456"
  },
  // Redis 缓存连接字符串
  "RedisSettings": {
    "ServiceIp": "127.0.0.1",
    "Port": 6379,
    "DbPwd": "123456"
  },
  // Swagger 配置
  "SwaggerSettings": {
    // 启用
    "Enable": true,
    "DocumentTitle": "Fast",
    "DocExpansionState": "None",
    "EnableAllGroups": true,
    "HideServers": true,
    // 生成Json文件枚举配置
    "EnumToNumber": true,
    "GroupOpenApiInfos": [
      {
        "Group": "FastCloud",
        "Title": "FastCloud",
        "Description": "一个持续集百家所长 众多黑科技的 .NET Web应用框架。<br/>前后端分离架构，开箱即用，紧随前沿技术。<br/>作者By 小方<br/><a href='https://gitee.com/China-xiaoFang/Fast.Admin'>https://gitee.com/China-xiaoFang/Fast.Admin</a>",
        "Version": "3.0.0",
        "Order": 999
      },
      {
        "Group": "Auth",
        "Title": "鉴权接口",
        "Description": "Fast.FastCloud 鉴权接口",
        "Version": "3.0.0",
        "Order": 998
      },
      {
        "Group": "FastCloudLog",
        "Title": "FastCloud 日志模块",
        "Description": "Fast.FastCloud 日志模块接口",
        "Version": "3.0.0",
        "Order": 997
      }
    ]
  }
}
