{
  // 数据库连接字符串
  "ConnectionSettings": {
    // 默认库连接Id
    "ConnectionId": "Fast",
    // 默认库服务器Ip
    "ServiceIp": "127.0.0.1",
    // 默认库端口号
    "Port": 1433,
    // 默认库数据库名称
    "DbName": "FastCenter",
    // 默认库数据库用户
    "DbUser": "admin",
    // 默认库数据库密码
    "DbPwd": "123456",
    // 自定义连接字符串
    "CustomConnectionStr": null,
    // 默认库服务器类型
    "DbType": "SqlServer",
    // 超时时间，单位秒
    "CommandTimeOut": 60,
    // SqlSugar Sql执行最大秒数，如果超过记录警告日志
    "SugarSqlExecMaxSeconds": 30,
    // 差异日志
    "DiffLog": true,
    // 禁用 SqlSugar 的 Aop
    "DisableAop": false
  },
  // Redis 缓存连接字符串
  "RedisSettings": {
    "ServiceIp": "127.0.0.1",
    "Port": 6379,
    "DbName": 1,
    "DbPwd": "123456",
    "Prefix": "Fast_",
    "Services": [
      {
        "ServiceName": "Auth",
        "DbName": 2
      },
      {
        "ServiceName": "Center",
        "DbName": 3
      }
    ]
  },
  // 雪花Id
  "SnowflakeSettings": {
    "WorkerId": 1
  }
}
